{
  "name": "server",
  "version": "0.0.1",
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon --exec ts-node --files -r tsconfig-paths/register ./src/index.ts",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node --files -r tsconfig-paths/register ./src/index.ts"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "jet-logger": "^2.0.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/find": "0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "5.1.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.21",
    "@types/react-native": "^0.72.2",
    "bcrypt": "^5.1.1",
    "express": "^4.21.0",
    "find": "0.3.0",
    "jasmine": "5.3.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "2.30.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "ts-command-line-args": "2.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
